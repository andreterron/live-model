{
  "name": "live-model",
  "version": "0.4.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vitest --watch --reporter verbose",
    "watch": "vite build --watch",
    "test": "vitest",
    "release": "rm -rf dist && pnpm build && pnpm publish",
    "prepack": "cp ../../README.md ."
  },
  "devDependencies": {
    "@testing-library/react": "16.1.0",
    "vitest": "^3.0.0",
    "@vitest/spy": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "zod": "^3.0.0"
  }
}